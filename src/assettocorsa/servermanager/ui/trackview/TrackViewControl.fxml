<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox minHeight="-Infinity" minWidth="-Infinity" onMouseClicked="#mouseClickOnTrack" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="trackName" text="Track Name" VBox.vgrow="ALWAYS">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <ImageView fx:id="trackImage" fitHeight="200.0" fitWidth="355.0" pickOnBounds="true" preserveRatio="true" />
      <GridPane alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="120.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
          <ColumnConstraints minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Pitboxes" />
            <Label fx:id="trackPitboxes" text="NumBoxes" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</VBox>
